@model PersonResults
@{
    ViewData["Title"] = "Delete Person";
}

<h2>@ViewBag.title</h2>

<p>Are you sure you want to delete Person: @Model.FirstName @Model.LastName</p>
<form asp-action="Delete" asp-route-id="@Model.Id">
    <a asp-action="Index" class="btn btn-secondary">No</a>
    <button type="submit" class="btn btn-danger">Yes</button>
</form>
